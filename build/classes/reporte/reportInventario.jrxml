	<queryString>
		<![CDATA[SELECT
     To_char(mp.fecha_inventario,'MM') AS Mes,
To_char(mp.fecha_inventario,'YYYY') AS a単o,     
mp."cantidad_disponible",
     m."nombre",
     l."nombre" AS planta
FROM
     "material_pedido" mp INNER JOIN "material_proveedor" mpr ON mp."fk_material_proveedor" = mpr."id"
